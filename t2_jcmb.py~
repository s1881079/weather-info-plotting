import TK2
import os

if __name__ == '__main__':
    
    #define the workpath
    workspace = '/home/s1881079/tigis/prac2'
    os.chdir(workspace)
    jcmb_name = 'plenty.data'
    
    #read JCMB file
    jcmb_data,jcmb_head = TK2.readJCMB(jcmb_name)
    
    #plot as demo
    sucPlot = TK2.picDemo(jcmb_data,jcmb_head)
    
    #wave Goodbye~
    TK2.goodBye(sucPlot)
    
    
    